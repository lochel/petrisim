<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <MenuBar id="menuBar" VBox.vgrow="NEVER">
            <menus>
                <fx:include fx:id="fileMenu" source="menu/FileMenu.fxml" />
                <fx:include fx:id="editMenu" source="menu/EditMenu.fxml" />
                <fx:include fx:id="simulationMenu" source="menu/SimulationMenu.fxml" />
                <fx:include fx:id="helpMenu" source="menu/HelpMenu.fxml" />
            </menus>
        </MenuBar>
        <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
            <tabs>
                <Tab text="Presentation">
                    <content>
                        <HBox spacing="5.0">
                            <children>
                                <VBox maxWidth="-Infinity" minWidth="-Infinity" HBox.hgrow="NEVER">
                                    <children>
                                        <fx:include source="editor/EditorTools.fxml" />
                                        <fx:include source="editor/SimulationControls.fxml" />
                                    </children>
                                </VBox>
                                <fx:include source="editor/GraphPane.fxml" />
                                <fx:include source="editor/ElementDetails.fxml" />
                            </children>
                            <padding>
                                <Insets left="5.0" right="5.0" top="5.0" />
                            </padding>
                        </HBox>
                    </content>
                </Tab>
                <Tab text="Node List">
                    <content>
                        <ListView />
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
            <children>
                <Label maxWidth="-1.0" text="Left status" HBox.hgrow="NEVER">
                    <font>
                        <Font size="11.0" fx:id="x3" />
                    </font>
                    <textFill>
                        <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
                    </textFill>
                    <HBox.margin>
                        <Insets />
                    </HBox.margin>
                </Label>
                <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
                <Label font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </HBox>
    </children>
</VBox>
