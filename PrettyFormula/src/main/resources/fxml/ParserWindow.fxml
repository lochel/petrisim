<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.embed.swing.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="15.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.unibi.agbi.prettyformulafx.main.PrettyFormulaGuiController">
    <children>
        <TextField fx:id="formulaInputTextField" minWidth="200.0" />
        <HBox alignment="CENTER" spacing="15.0">
            <children>
                <Button fx:id="formulaButtonSave" mnemonicParsing="false" text="OK" />
                <Button fx:id="formulaButtonCancel" mnemonicParsing="false" text="Cancel" />
            </children>
        </HBox>
        <Label fx:id="formulaErrorLabel" text="errors..." />
      <ScrollPane fitToHeight="true">
         <content>
              <AnchorPane>
                  <children>
                      <SwingNode fx:id="formulaSwingNode" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                  </children>
              </AnchorPane>
         </content>
      </ScrollPane>
      <VBox VBox.vgrow="ALWAYS" />
    </children>
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
    </padding>
</VBox>
